{
    "name": "resto2web/menu",
    "description": "Package for Resto2Web menu functionalities",
    "keywords": [
        "resto2web",
        "menu"
    ],
    "homepage": "https://github.com/resto2web/menu",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Lucas Houssa",
            "email": "lucas@retinens.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "bumbummen99/shoppingcart": "^3.0",
        "illuminate/support": "^8.0",
        "laravelcollective/html": "^6.2",
        "resto2web/admin": "dev-develop",
        "resto2web/core": "dev-develop",
        "spatie/data-transfer-object": "^2.5",
        "spatie/laravel-model-states": "^2.0",
        "spatie/laravel-settings": "^1.0"
    },
    "require-dev": {
        "orchestra/canvas": "^6.1",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Resto2web\\Menu\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Resto2web\\Menu\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Resto2web\\Menu\\MenuServiceProvider"
            ],
            "aliases": {
                "Menu": "Resto2web\\Menu\\MenuFacade"
            }
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../core"
        },
        {
            "type": "path",
            "url": "../admin"
        }
    ]

}
